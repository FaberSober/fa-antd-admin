{
  "presets": [
    [
      "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": {
            "version": "3.0"
          },
          "targets":{
            "chrome": 60,
            "firefox": 60,
            "ie": 9,
            "safari": 10,
            "edge": 17
          }
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime"],
    ["@babel/plugin-syntax-dynamic-import"]
  ]
}
